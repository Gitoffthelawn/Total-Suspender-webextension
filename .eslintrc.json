{
  "extends": [
      "airbnb-base",
      "plugin:jest/recommended"
  ],
  "plugins": [
      "jsx",
      "jest"
  ],
  "env": {
      "browser": true,
      "webextensions": true,
      "jest": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "jsx/mark-used-vars": 2
  },
  "parser": "babel-eslint"
}