{
  "applications": {
    "gecko": {
      "strict_min_version": "62.0"
    }
  },
  "background": {
      "page": "background/background.html"
  },
  "browser_action": {
    "browser_style": true,
    "default_title": "Total Suspender",
    "default_popup": "options/options.html",
    "default_icon": "icons/iconfinder_shutdown_2639921.svg"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "icons": {
    "48": "icons/iconfinder_shutdown_2639921.png"
  },
  "description": "Suspends all inactive tabs",
  "homepage_url": "https://github.com/k5md/Total-Suspender-webextension",
  "manifest_version": 2,
  "name": "Total Suspender",
  "version": "2.8"
}